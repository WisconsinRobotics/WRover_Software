<!-- Launches the drive system teleoperation logic node. -->
<launch>
	<group ns="logic">
		<group ns="drive_system">
			<node name="logic_teleop" pkg="wr_logic_teleop_ds" type="TeleopDriveTrainLogic"
				machine="wrover">
				<param name="speed_step1" value="0.0" />
				<param name="speed_step2" value="0.25" />
				<param name="speed_step3" value="0.25" />
				<param name="speed_step4" value="0.25" />
			</node>
		</group>
	</group>
</launch>