<launch>
    <group ns="control">
        <node name="WResponseControl_ArmMotors" pkg="wresponsecontrol" type="WResponseControl">
            <rosparam file="$(find wr_control_drive_arm)/config/arm_motor_PID.yaml"/>
        </node>
        <node name="ArmControlSystem" pkg="wr_control_drive_arm" type="ArmControlSystem" output="screen">
            <rosparam file="$(find wr_control_drive_arm)/config/encoder_parameters.yaml" if="$(eval env('WROVER_HW') == 'REAL')"/>
            <rosparam file="$(find wr_control_drive_arm)/config/mocked_encoder_parameters.yaml" if="$(eval env('WROVER_HW') == 'MOCK')"/>
        </node>
    </group>
</launch>
